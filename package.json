    "prisma:migrate": "prisma migrate dev --preview-feature",
    "seeder:seed": "ts-node seeders/seed.ts",
    "clean": "rimraf dist"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma",
    "seed": "tsx src/prisma/seed/seed.ts"
  },
